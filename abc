import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
		System.out.println("Введите два числа");
		Scanner scan = new Scanner(System.in);
		Scanner skam = new Scanner(System.in);
		int a = scan.nextInt();
		int b = skam.nextInt();
		System.out.println ("Ответ: " + (a+b));
	}

